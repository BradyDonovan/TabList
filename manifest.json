{
    "manifest_version": 3,
    "name": "TabList",
    "version": "1.0",
    "description": "A browser extension to get a list of your tabs at the click of a button, separated by newline characters.",
    "icons": {
        "48": "icons/tablist.png"
    },
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_icon": "icons/tablist.png",
        "default_title": "TabList",
        "default_popup": "./src/tablist/tablist.html"
    }
}